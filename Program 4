// Program 4: Input and Display Date of Birth
#include <stdio.h>
#include <conio.h>

int main() 
{
    int dd, mm, yyyy;
    clrscr();
    printf("Enter Date of Birth: ");
    scanf("%d-%d-%d", &dd, &mm, &yyyy);
    printf("In Date of Birth:\n dd=%d \n mm=%d \n yyyy=%d", dd, mm, yyyy);
    getch();
    return 0;
}
